<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>UserRegister</servlet-name>
        <servlet-class>com.sun.servlet.UserRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRegister</servlet-name>
        <url-pattern>/UserRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Userlogin</servlet-name>
        <servlet-class>com.sun.servlet.Userlogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Userlogin</servlet-name>
        <url-pattern>/Userlogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addLike</servlet-name>
        <servlet-class>com.sun.servlet.addLike</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addLike</servlet-name>
        <url-pattern>/addLike</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>recommend</servlet-name>
        <servlet-class>com.sun.servlet.recommend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>recommend</servlet-name>
        <url-pattern>/recommend</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>likeList</servlet-name>
        <servlet-class>com.sun.servlet.likeList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>likeList</servlet-name>
        <url-pattern>/likeList</url-pattern>
    </servlet-mapping>
</web-app>